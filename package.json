{
  "name": "doragonabe.github.io",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxi dev --dotenv .env.local -o",
    "dev:github": "nuxi dev --dotenv .env.github -o",
    "tsc": "vue-tsc --noEmit",
    "typecheck": "nuxi typecheck",
    "build": "nuxi build",
    "generate:local": "nuxi generate --dotenv .env.local",
    "generate:github": "nuxi generate --dotenv .env.github",
    "preview": "nuxi preview",
    "test:unit": "vitest run app/tests/unit",
    "test:unit:watch": "vitest watch app/tests/unit",
    "test:unit:coverage": "vitest run app/tests/unit --coverage",
    "test:e2e": "playwright test app/tests/pages",
    "test:e2e:ui": "pnpm dlx playwright test app/tests/pages --ui",
    "test:e2e:report": "pnpm dlx playwright show-report",
    "preinstall": "pnpm dlx only-allow pnpm",
    "postinstall": "nuxi prepare",
    "analyze": "pnpm dlx nuxi analyze",
    "cleanup": "pnpm dlx nuxi cleanup",
    "info": "pnpm dlx nuxi info",
    "lint:code": "biome lint ./",
    "lint:style": "biome format ./",
    "fix:style": "biome format --write ./",
    "fix:code": "biome lint --apply ./",
    "outdated": "pnpm outdated",
    "audit": "pnpm audit"
  },
  "dependencies": {
    "@pinia/nuxt": "0.11.2",
    "bignumber.js": "^9.3.1",
    "bowser": "^2.12.1",
    "dayjs": "^1.11.18",
    "dayjs-nuxt": "^2.1.11",
    "es-toolkit": "^1.39.10",
    "mermaid": "^11.12.0",
    "nuxt": "^4.1.2",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "qrcode": "^1.5.4",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.4",
    "@playwright/test": "^1.55.0",
    "@testing-library/vue": "^8.1.0",
    "@types/sanitize-html": "^2.16.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^12.8.2",
    "@vueuse/integrations": "^13.9.0",
    "@vueuse/nuxt": "^12.8.2",
    "happy-dom": "^17.6.3",
    "nuxt-security": "^2.4.0",
    "nuxt-swiper": "^2.0.1",
    "nuxt-typed-router": "3.7.3",
    "playwright-core": "^1.55.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sanitize-html": "^2.17.0",
    "sass": "^1.92.1",
    "vite": "npm:rolldown-vite@^7.1.11",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "vue-demi"
    ],
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    }
  }
}
