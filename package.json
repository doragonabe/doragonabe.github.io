{
  "name": "doragonabe.github.io",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxi dev --dotenv .env.local -o",
    "tsc": "vue-tsc --noEmit",
    "typecheck": "nuxi typecheck",
    "build": "nuxi build",
    "generate:local": "nuxi generate --dotenv .env.local",
    "generate:github": "nuxi generate --dotenv .env.github",
    "preview": "nuxi preview",
    "test:unit": "vitest run tests/unit",
    "test:unit:watch": "vitest watch tests/unit",
    "test:unit:coverage": "vitest run tests/unit --coverage",
    "test:e2e": "playwright test tests/pages",
    "test:e2e:ui": "pnpx playwright test tests/pages --ui",
    "test:e2e:report": "pnpx playwright show-report",
    "preinstall": "pnpx only-allow pnpm",
    "postinstall": "nuxi prepare",
    "analyze": "pnpx nuxi analyze",
    "cleanup": "pnpx nuxi cleanup",
    "info": "pnpx nuxi info",
    "lint:code": "biome lint ./",
    "lint:style": "biome format ./",
    "fix:style": "biome format --write ./",
    "fix:code": "biome lint --apply ./",
    "outdated": "pnpm outdated",
    "audit": "pnpm audit"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "bowser": "^2.11.0",
    "dayjs": "^1.11.13",
    "dayjs-nuxt": "^2.1.11",
    "nuxt": "^3.16.0",
    "swiper": "^11.2.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@headlessui/tailwindcss": "^0.2.2",
    "@nuxt/test-utils": "^3.15.4",
    "@nuxt/ui": "^2.21.0",
    "@playwright/test": "^1.50.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/sanitize-html": "^2.13.0",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^12.5.0",
    "@vueuse/nuxt": "^12.5.0",
    "nuxt-lodash": "^2.5.3",
    "nuxt-security": "^2.1.5",
    "nuxt-swiper": "^2.0.0",
    "nuxt-typed-router": "3.7.3",
    "prettier": "^3.5.0",
    "sanitize-html": "^2.14.0",
    "sass-embedded": "^1.83.4",
    "tailwindcss": "^3.2.7",
    "vitest": "^3.0.5",
    "vue-tsc": "2.1.6"
  },
  "browserslist": ["> 1%", "last 2 versions", "not dead", "not ie 11"],
  "engines": {
    "node": "^23.0.0",
    "pnpm": "^9.0.0"
  },
  "packageManager": "pnpm@9.15.2"
